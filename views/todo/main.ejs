<div>
    <h1>Todo List</h1>
    <% users.forEach(user => { %>
    <div class="card">
       <div class="card-body">
           <h5 class="card-title"><%= user.title%></h5>
           <h6 class="card-subtitle"><%= user.description%></h6>
            <button
                onclick=""
                class="btn btn-warning">
                Completed
            </button>
           <%})%>
       </div>
    </div>
</div>

<div>
    <a href="/signin" class="btn btn-primary btn-lg active" role="button" aria-pressed="true">Sign Out</a>
</div>

